{
  "name": "ember-wired",
  "version": "1.0.3",
  "description": "A wrapper library for WiredJS web components.",
  "keywords": [
    "ember-addon",
    "webcomponent",
    "web component",
    "rough",
    "sketchy",
    "hand-drawn",
    "hand drawn",
    "wireframe"
  ],
  "homepage": "https://egaba.github.io/ember-wired/",
  "repository": {
    "type": "git",
    "url": "https://github.com/egaba/ember-wired.git"
  },
  "license": "MIT",
  "author": "Edward Gaba <egaba88@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "deploy": "rm -rf docs && ember build -prod --output-path docs && git add . && git commit -am 'deploy gh page' && git push --follow-tags"
  },
  "peerDependencies": {
    "wired-elements": "1.x"
  },
  "dependencies": {
    "ember-cli-babel": "^7.4.1",
    "ember-cli-htmlbars": "^3.0.1",
    "wired-elements": "^1.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.10.0-beta.2",
    "ember-cli-cjs-transform": "^1.3.1",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.10.0-beta.1",
    "ember-source-channel-url": "^1.1.0",
    "ember-truth-helpers": "^2.1.0",
    "ember-try": "^1.0.0",
    "eslint-plugin-ember": "^6.2.0",
    "eslint-plugin-node": "^8.0.1",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.8.4"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://egaba.github.io/ember-wired/"
  }
}
